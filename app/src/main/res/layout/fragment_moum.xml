<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linear"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="30dp"
    android:background="?attr/selectableItemBackground"
    android:clickable="true"
    android:padding="15dp"
    tools:context=".moum">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/side_line3"
        android:orientation="vertical"
        android:padding="3dp">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:stretchColumns="*">

            <TableRow>

                <Button
                    android:id="@+id/k"
                    android:layout_height="120dp"
                    android:background="@drawable/side_line2"
                    android:foreground="?selectableItemBackground"
                    android:text="ㅏ"
                    android:textSize="50dp"
                    android:textStyle="bold" />

                <Button
                    android:id="@+id/i"
                    android:layout_height="120dp"
                    android:background="@drawable/side_line2"
                    android:foreground="?selectableItemBackground"
                    android:text="ㅑ"
                    android:textSize="50dp"
                    android:textStyle="bold" />

                <Button
                    android:id="@+id/j"
                    android:layout_height="120dp"
                    android:background="@drawable/side_line2"
                    android:foreground="?selectableItemBackground"
                    android:text="ㅓ"
                    android:textSize="50dp"
                    android:textStyle="bold" />
            </TableRow>

            <TableRow>

                <Button
                    android:id="@+id/u"
                    android:layout_height="120dp"
                    android:background="@drawable/side_line2"
                    android:foreground="?selectableItemBackground"
                    android:text="ㅕ"
                    android:textSize="50dp"
                    android:textStyle="bold" />

                <Button
                    android:id="@+id/h"
                    android:layout_height="120dp"
                    android:background="@drawable/side_line2"
                    android:foreground="?selectableItemBackground"
                    android:text="ㅗ"
                    android:textSize="50dp"
                    android:textStyle="bold" />

                <Button
                    android:id="@+id/y"
                    android:layout_height="120dp"
                    android:background="@drawable/side_line2"
                    android:foreground="?selectableItemBackground"
                    android:text="ㅛ"
                    android:textSize="50dp"
                    android:textStyle="bold" />
            </TableRow>

            <TableRow>

                <Button
                    android:id="@+id/n"
                    android:layout_height="120dp"
                    android:background="@drawable/side_line2"
                    android:foreground="?selectableItemBackground"
                    android:text="ㅜ"
                    android:textSize="50dp"
                    android:textStyle="bold" />

                <Button
                    android:id="@+id/b"
                    android:layout_height="120dp"
                    android:background="@drawable/side_line2"
                    android:foreground="?selectableItemBackground"
                    android:text="ㅠ"
                    android:textSize="50dp"
                    android:textStyle="bold" />

                <Button
                    android:id="@+id/m"
                    android:layout_height="120dp"
                    android:background="@drawable/side_line2"
                    android:foreground="?selectableItemBackground"
                    android:text="ㅡ"
                    android:textSize="50dp"
                    android:textStyle="bold" />
            </TableRow>

            <TableRow>

                <Button
                    android:id="@+id/l"
                    android:layout_height="120dp"
                    android:background="@drawable/side_line2"
                    android:foreground="?selectableItemBackground"
                    android:text="ㅣ"
                    android:textSize="50dp"
                    android:textStyle="bold" />

                <Button
                    android:layout_height="match_parent"
                    android:background="@drawable/side_line2"
                    android:enabled="false" />

                <Button
                    android:layout_height="match_parent"
                    android:background="@drawable/side_line2"
                    android:enabled="false" />
            </TableRow>
        </TableLayout>
    </LinearLayout>
</FrameLayout>